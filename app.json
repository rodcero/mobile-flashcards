{
  "expo": {
    "name": "mobile-flashcards",
    "slug": "mobile-flashcards",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
